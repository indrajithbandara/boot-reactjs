{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-console": [0],
    "indent": [2, 2],
    "comma-dangle": [2, "never"],
    "react/prop-types": 0,
    "react/no-danger": 0
    },
  "plugins": [
    "react"
  ]
}
